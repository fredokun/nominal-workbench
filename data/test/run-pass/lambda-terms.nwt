# To use with "lambda-calculus.nw"

# Id
Lambda(x, Var(x))

# Church Integers
# Zero
Lambda([f], Lambda([x], Var(x)))

# One
Lambda([f], Lambda([x], App(Var(f), Var(x))))

# Two
Lambda([f], Lambda([x], App(Var(f), (App(Var(f), Var(x))))))

# Succ
Lambda([n], Lambda([f], Lambda([x], 
  App(Var(f), 
      (App(Var(n), (App(Var(f), Var(x)))))))))

